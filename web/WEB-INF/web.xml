        <servlet-class>khanhhq.servlet.UserTestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestServlet</servlet-name>
        <servlet-class>khanhhq.servlet.TestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HistoryUserServlet</servlet-name>
        <servlet-class>khanhhq.servlet.HistoryUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchHistoryServlet</servlet-name>
        <servlet-class>khanhhq.servlet.SearchHistoryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchHistory</servlet-name>
        <servlet-class>searchHistory</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>displayUser</servlet-name>
        <url-pattern>/displayUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrintDataAdminServlet</servlet-name>
        <url-pattern>/PrintDataAdminServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrintDataUserServlet</servlet-name>
        <url-pattern>/PrintDataUserServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOutServlet</servlet-name>
        <url-pattern>/LogOutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateAccountServlet</servlet-name>
        <url-pattern>/CreateAccountServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchQuestionNameServlet</servlet-name>
        <url-pattern>/SearchQuestionNameServlet</url-pattern>
    </servlet-mapping>
