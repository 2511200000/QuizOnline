<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
      <filter>
        <filter-name>FilterDispatcher</filter-name>
        <filter-class>khanhhq.filter.FilterDispatcher</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>FilterDispatcher</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>khanhhq.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>displayUser</servlet-name>
        <servlet-class>displayUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PrintDataAdminServlet</servlet-name>
        <servlet-class>khanhhq.servlet.PrintDataAdminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PrintDataUserServlet</servlet-name>
        <servlet-class>khanhhq.servlet.PrintDataUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOutServlet</servlet-name>
        <servlet-class>khanhhq.servlet.LogOutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateAccountServlet</servlet-name>
        <servlet-class>khanhhq.servlet.CreateAccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchQuestionNameServlet</servlet-name>
        <servlet-class>khanhhq.servlet.SearchQuestionNameServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateQuestionServlet</servlet-name>
        <servlet-class>khanhhq.servlet.CreateQuestionServlet</servlet-class>
